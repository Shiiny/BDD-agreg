{{ form_start(form) }}
    <div class="row">
        <div class="col-lg-12">
            {{ form_row(form.title) }}
        </div>
    </div>
    <div class="row">
        <div class="col-lg-4">
            {{ form_row(form.id_moodle) }}
        </div>
        <div class="col-lg-4">
            {{ form_row(form.discipline) }}
        </div>
        <div class="col-lg-4">
            {{ form_row(form.hours) }}
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            {{ form_row(form.teachers) }}
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">
            {{ form_row(form.concours) }}
        </div>
    </div>
    <button class="btn btn-primary">{{ button|default('Ajouter') }}</button>
{{ form_rest(form) }}
{{ form_end(form) }}
