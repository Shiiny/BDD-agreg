{{ form_start(form) }}
<div class="row">
    <div class="col-lg-5">
        {{ form_row(form.lastname) }}
    </div>
    <div class="col-lg-5">
        {{ form_row(form.firstname) }}
    </div>
    <div class="col-lg-2">
        {{ form_row(form.idMoodle) }}
    </div>
</div>
<div class="row">
    <div class="col-lg-5">
        {{ form_row(form.email) }}
    </div>
    <div class="col-lg-3">
        {{ form_row(form.phone) }}
    </div>
    <div class="col-lg-4">
        {{ form_row(form.concours) }}
    </div>
</div>
<div class="row">
    <div class="col-lg-12">
        {{ form_row(form.discipline) }}
    </div>
</div>

    <button class="btn btn-primary">{{ button|default('Ajouter') }}</button>
{{ form_rest(form) }}
{{ form_end(form) }}
